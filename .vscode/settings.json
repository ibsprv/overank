{
  "editor.formatOnSave": true,
  "prettier.eslintIntegration": true,
  "prettier.jsxBracketSameLine": true,
  "files.exclude": {
    "**/.DS_Store": true,
    "**/node_modules/": true,
    "**/build/**/*.js.map": { "when": "$(basename)" },
    "**/build/**/*.js": { "when": "$(basename).ts" }
  }
}
